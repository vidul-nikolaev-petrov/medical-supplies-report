{
    "name": "medsupplies",
    "productName": "Med Supplies",
    "executableName": "Med Supplies",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": ".webpack/main",
    "scripts": {
        "start": "NODE_ENV=dev electron-forge start",
        "package": "electron-forge package",
        "make": "NODE_ENV=prod electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx .",
        "test": "jest"
    },
    "keywords": [],
    "author": {
        "name": "Vidul Nikolaev Petrov",
        "email": "vidul.petrov@gmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "my_new_app"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/renderer.ts",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.59",
        "@electron-forge/maker-deb": "^6.0.0-beta.59",
        "@electron-forge/maker-rpm": "^6.0.0-beta.59",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
        "@electron-forge/maker-zip": "^6.0.0-beta.59",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.59",
        "@types/jest": "^27.0.1",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "@vercel/webpack-asset-relocator-loader": "^1.6.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.2.0",
        "diff": "^5.0.0",
        "electron": "^13.6.3",
        "electron-packager": "^15.3.0",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.24.2",
        "fork-ts-checker-webpack-plugin": "^6.3.2",
        "jest": "^27.1.0",
        "node-loader": "^2.0.0",
        "style-loader": "^3.2.1",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.5",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "@rehooks/online-status": "^1.1.2",
        "@types/react-router-dom": "^5.3.3",
        "electron-log": "^4.4.1",
        "electron-squirrel-startup": "^1.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.2"
    }
}
